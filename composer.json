{
	"require": {
		"laravel/framework": "4.1.*",
		"cartalyst/sentry": "~2.1",
		"stolz/assets": "dev-master"
	},
	"require-dev": {
		"way/generators": "~2.6",
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0"
	},
	"repositories": [
    {
        "type":"vcs",
        "url": "https://github.com/SeanBuller/Assets"
    }
	],
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Authority": "app/"
		}
	},
	"scripts": {
		"post-update-cmd": "php artisan optimize"
	},
	"minimum-stability": "dev"
}
